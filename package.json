{
  "name": "we-are-developers-2024-jsii-sample",
  "version": "1.0.1",
  "description": "A demonstration jsii library",
  "main": "lib/index.ts",
  "bundleDependencies": ["moment"],
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii --watch",
    "package": "jsii-pacmak"
  },
  "keywords": [],
  "author": {
    "name": "Markus Ziller",
    "email": "kontakt@markusziller.de"
  },
  "repository": {
    "url": "https://github.com/markusz/we-are-developers-2024-jsii-sample.git"
  },
  "license": "ISC",
  "stability": "experimental",
  "types": "dist/lib/index.d.ts",
  "jsii": {
    "versionFormat": "full",
    "tsc": {
      "outDir": "dist"
    },
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "hello_jsii",
        "module": "hello_jsii"
      },
      "java": {
        "package": "acme.jsii.hello",
        "maven": {
          "groupId": "acme",
          "artifactId": "jsii-hello"
        }
      }
    }
  },
  "dependencies": {
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45"

  },
  "devDependencies": {
    "jsii": "^5.4.25",
    "jsii-pacmak": "^1.101.0",
    "@types/node": "^20.14.9",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "@types/jest": "^29.4.0"
  }
}
